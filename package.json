    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
    "standard": "17.1.0",
    "svelte": "4.2.0",
    "svelte-check": "3.5.1",
    "tailwindcss": "3.3.3",
